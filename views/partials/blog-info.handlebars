<article>
  <li class="blog-item">
      <div class="blog-title">
          <a href="/blog/{{id}}" target="_blank">{{title}}</a>
          <span>Posted by: {{user.username}} on {{formatDate created_at}}</span>
      </div>
      <div class="blog-body">
          <p>{{body}}</p>
      </div>
  </li>
 </article>